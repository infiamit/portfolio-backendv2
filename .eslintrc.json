{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": ["eslint:recommended", "airbnb-base"],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "ecmaFeatures": {
            "impliedStrict": true
          }
    },
    "rules": {
          "linebreak-style":"off",
          "no-underscore-dangle":"off",
          "max-len":"off",
        "indent": [
            "error",
            4
        ]
    }
}